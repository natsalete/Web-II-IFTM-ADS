{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "CarrosApplication",
      "request": "launch",
      "mainClass": "com.iftm.projeto.carros.CarrosApplication",
      "projectName": "carros"
    },
    {
      "type": "java",
      "name": "DemoApplication",
      "request": "launch",
      "mainClass": "com.example.DemoApplication",
      "projectName": "demo"
    },
    {
      "type": "totvs_language_debug",
      "request": "launch",
      "name": "TOTVS Language Debug",
      "program": "${command:AskForProgramName}",
      "cwb": "${workspaceFolder}",
      "smartclientBin": "../totvs/bin/smartclient/smartclient.exe",
      "isMultiSession": true,
      "enableTableSync": true,
      "extendFeatures": {
        "charDetails": false
      }
    }
  ]
}
